{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",
  "name": "Example Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "languageAdapters": {
    "kotlin": "net.fabricmc.language.kotlin.KotlinAdapter"
  },
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://legacyfabric.net/",
    "sources": "https://github.com/Legacy-Fabric/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "net.fabricmc.example.ExampleMod"
      }
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.19",
    "minecraft": "1.8.9",
    "fabric-language-kotlin": ">=1.10.17+kotlin.1.9.22"
  },
  "suggests": {
    "another-mod": "*"
  }
}
